<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">
	<application>
		<locale-config>
			<default-locale>pt_BR</default-locale>
			<supported-locale>es_ES</supported-locale>
			<supported-locale>en_US</supported-locale>
			<supported-locale>pt_BR</supported-locale>			
		</locale-config>
		<resource-bundle>
			<base-name>resources.application</base-name>
			<var>app</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>resources.messages</base-name>
			<var>rotulo</var>
		</resource-bundle>
	</application>
	<managed-bean>
		<managed-bean-name>languageController</managed-bean-name>
		<managed-bean-class>br.com.supportcomm.mktcall.managedbean.LanguageController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loginMBean</managed-bean-name>
		<managed-bean-class>br.com.supportcomm.mktcall.managedbean.LoginMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>agenciaMBean</managed-bean-name>
		<managed-bean-class>br.com.supportcomm.mktcall.managedbean.AgenciaMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>anuncianteMBean</managed-bean-name>
		<managed-bean-class>br.com.supportcomm.mktcall.managedbean.AnuncianteMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>campanhaMBean</managed-bean-name>
		<managed-bean-class>br.com.supportcomm.mktcall.managedbean.CampanhaMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>listaFidelizadaMBean</managed-bean-name>
		<managed-bean-class>br.com.supportcomm.mktcall.managedbean.ListaFidelizadaMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>relatorioMBean</managed-bean-name>
		<managed-bean-class>br.com.supportcomm.mktcall.managedbean.RelatorioMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>spotMBean</managed-bean-name>
		<managed-bean-class>br.com.supportcomm.mktcall.managedbean.SpotMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
		<managed-bean>
		<managed-bean-name>campanhaSimuladorMBean</managed-bean-name>
		<managed-bean-class>br.com.supportcomm.mktcall.managedbean.CampanhaSimuladorMBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<lifecycle>
		<phase-listener>br.com.supportcomm.mktcall.system.ControleUsuarioListener</phase-listener>
	</lifecycle>
	<navigation-rule>
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>usuario</from-outcome>
			<to-view-id>/paginas/usuario.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>termos</from-outcome>
			<to-view-id>/paginas/termos.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>spot</from-outcome>
			<to-view-id>/paginas/spot.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>seguranca</from-outcome>
			<to-view-id>/paginas/seguranca.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>relatorios</from-outcome>
			<to-view-id>/paginas/relatorios.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>listaFidelizada</from-outcome>
			<to-view-id>/paginas/listaFidelizada.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>listaSegmentada</from-outcome>
			<to-view-id>/paginas/listaSegmentada.xhtml</to-view-id>
		</navigation-case>
		
		
		
		<navigation-case>
			<from-outcome>graficos</from-outcome>
			<to-view-id>/paginas/graficos.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>campanha</from-outcome>
			<to-view-id>/paginas/campanha.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>anunciante</from-outcome>
			<to-view-id>/paginas/anunciante.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>agencia</from-outcome>
			<to-view-id>/paginas/agencia.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>relatorioEfetividade</from-outcome>
			<to-view-id>/paginas/reports/efetividade.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>relatorioIntegracao</from-outcome>
			<to-view-id>/paginas/reports/interacao.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>relatorioDuracao</from-outcome>
			<to-view-id>/paginas/reports/duracao.xhtml</to-view-id>
		</navigation-case>

		<navigation-case>
			<from-outcome>simulador</from-outcome>
			<to-view-id>/paginas/simulador.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>controleDialOut</from-outcome>
			<to-view-id>/paginas/controleDialOut.xhtml</to-view-id>
		</navigation-case>
		
		
	</navigation-rule>
</faces-config>
